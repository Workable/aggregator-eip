{
  "name": "aggregator-eip",
  "description": "Aggregattor Enterprise Integration Module",
  "version": "0.1.1",
  "dependencies": {
    "amqplib": "^0.3.2",
    "async": "^1.2.0",
    "co": "^4.5.4",
    "kerberos": "0.0.17",
    "log4js": "^0.6.25",
    "mongoose": "^4.2.7",
    "uuid": "^2.0.1"
  },
  "scripts": {
    "lint": "jshint .",
    "checkStyle": "jscs . -e",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "node --harmony node_modules/istanbul-harmony/lib/cli.js cover ./node_modules/vows/bin/vows test/* --spec",
    "test_new": "AGGREGATOR_TIMEOUT_DELAY=10 NODE_ENV=test PORT=9999 node --harmony node_modules/istanbul-harmony/lib/cli.js test ./node_modules/.bin/_mocha test/* --  -R spec -r co-mocha",
    "posttest": "istanbul check-coverage --statements 85 --branches 70 --functions 100 --lines 90 && rm -rf coverage",
    "console": "node --harmony console.js",
    "prepack": "rm *.tgz",
    "pack": "npm pack"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": " <nikosd23@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "co-mocha": "^1.1.0",
    "istanbul": "^0.3.8",
    "istanbul-harmony": "^0.3.12",
    "jscs": "^1.13.1",
    "jshint": "^2.5.6",
    "mocha": "^2.2.1",
    "mongoose-fixtures": "0.0.1",
    "repl.history": "^0.1.3",
    "should": "^6.0.3",
    "sinon": "^1.14.1",
    "vows": "^0.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nikosd23/aggregator-eip.git"
  }
}
